{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "orle",
  "title": "Orle - Developer Tools",
  "type": "module",
  "description": "100+ developer tools: URL encode, JSON format, UUID, hashing, and more. All processing happens locally.",
  "icon": "extension-icon.png",
  "author": "punit_arani",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-tools",
      "title": "Developer Tools Hub",
      "subtitle": "orle.dev",
      "description": "Search and use 100+ developer tools",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.93.3",
    "@iarna/toml": "^2.2.5",
    "@noble/hashes": "^2.0.1",
    "@paralleldrive/cuid2": "^3.0.4",
    "@scure/base": "^2.0.0",
    "bech32": "^2.0.0",
    "cronstrue": "^3.9.0",
    "culori": "^4.0.2",
    "fast-xml-parser": "^5.3.3",
    "jsonpath-plus": "^10.3.0",
    "lucide-react": "^0.562.0",
    "marked": "^17.0.1",
    "nanoid": "^5.1.6",
    "papaparse": "^5.5.3",
    "prettier": "^3.7.4",
    "zod": "^4.2.1",
    "punycode": "^2.3.1",
    "sql-formatter": "^15.6.12",
    "terser": "^5.44.1",
    "turndown": "^7.2.2",
    "ulid": "^3.0.2",
    "uuid": "^13.0.0",
    "js-yaml": "^4.1.1",
    "diff": "^8.0.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/diff": "^7.0.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "22.15.29",
    "@types/react": "19.1.6",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.29.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "packageManager": "npm"
}
